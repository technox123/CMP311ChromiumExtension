{
  "manifest_version": 2,
  "name": "Password Helper",
  "version": "0.1.0",
  "permissions": [
    "<all_urls>",
    "unlimitedStorage",
    "storage"
  ],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "browser_action": {
    "default_popup": "PasswordStr.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
}
